                    Console.Write(Count + "-тридцать ");
                    break;
                case 31:
                    Console.Write(Count + "-тридцать одно");
                    break;
                case 32:
                    Console.Write(Count + "-тридцать два");
                    break;
                case 33:
                    Console.Write(Count + "-тридцать три");
                    break;
                case 34:
                    Console.Write(Count + "-тридцать четныре");
                    break;
                case 35:
                    Console.Write(Count + "-тридцать пять");
