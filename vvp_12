                Console.WriteLine("десять");
            if (A % 100 == 11)
                Console.WriteLine("одиннадцать");
            if (A % 100 == 12)
                Console.WriteLine("двенадцать");
            if (A % 100 == 13)
                Console.WriteLine("тринадцать");
