            Console.WriteLine("Дано целое число в диапазоне 100–999. Вывести строку-описание данного числа, например: 256 — «двести пятьдесят шесть», 814 — «восемьсот четырнадцать».");
            Console.WriteLine("Введите трехзначное число");
            int A = int.Parse(Console.ReadLine());
            if (A / 100==1)
                Console.WriteLine("сто");
            if (A / 100 == 2)
                Console.WriteLine("двести");
            if (A / 100 == 3)
                Console.WriteLine("тристо");
            if (A / 100 == 4)
                Console.WriteLine("четыресто");
            if (A / 100 == 5)
                Console.WriteLine("пятьсот");
            if (A / 100 == 6)
                Console.WriteLine("шестсот");
            if (A / 100 == 7)
                Console.WriteLine("семьсот");
