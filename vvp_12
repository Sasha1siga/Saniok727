                Console.WriteLine("двадцать");
            if ((A % 100) / 10 == 3)
                Console.WriteLine("тридцать");
            if ((A % 100) / 10 == 4)
                Console.WriteLine("сорок");
            if ((A % 100) / 10 == 5)
                Console.WriteLine("пятьдесят");
            if ((A % 100) / 10 ==6)
                Console.WriteLine("шестьдесят");
            if ((A % 100) / 10 == 7)
                Console.WriteLine("семьдесят");
            if ((A % 100) / 10 == 8)
                Console.WriteLine("восемьдесят");
            if ((A % 100) / 10 == 9)
                Console.WriteLine("девяносто");

            if (A % 10 == 1 && (A % 100) /10 != 1)
                Console.WriteLine("один");
            if (A % 10 == 2 && (A % 100) / 10 != 1)
                Console.WriteLine("два");
            if (A % 10 == 3 && (A % 100) / 10 != 1)
                Console.WriteLine("три");
            if (A % 10 == 4 && (A % 100) / 10 != 1)
                Console.WriteLine("четыре");
            if (A % 10 == 5 && (A % 100) / 10 != 1)
                Console.WriteLine("пять");
            if (A % 10 == 6 && (A % 100) / 10 != 1)
                Console.WriteLine("шесть");
            if (A % 10 == 7 && (A % 100) / 10 != 1)
                Console.WriteLine("семь");
            if (A % 10 == 8 && (A % 100) / 10 != 1)
                Console.WriteLine("восемь");
            if (A % 10 == 9 && (A % 100) / 10 != 1)
                Console.WriteLine("девять");
            Console.WriteLine();


            Console.WriteLine("В восточном календаре принят 60-летний цикл, состоящий из 12- летних подциклов, обозначаемых названиями цвета: зеленый, красный, желтый, белый и черный. В каждом подцикле годы носят названия животных: крысы, коровы, тигра, зайца, дракона, змеи, лошади, овцы, обезьяны, курицы, собаки и свиньи. По номеру года определить его название, если 1984 год — начало цикла: «год зеленой крысы».");
            Console.WriteLine("Введите номер года");
            int year = int.Parse(Console.ReadLine()); 
            int razn = Math.Abs(1984 - year)%60;
            int color = razn / 12;
            int pet = razn-(razn / 12)*12;
            Console.Write("год ");
            if (color+1 == 1 )
                Console.Write("зеленой(ого) ");
            if (color + 1 == 2)
                Console.Write("красной(ого) ");
            if (color + 1 == 3)
                Console.Write("желтой(ого) ");
            if (color + 1 == 4)
                Console.Write("белой(ого) ");
            if (color + 1 == 5)
                Console.Write("черной(ого) ");

            if(pet + 1 == 1)
                Console.Write("крысы");
            if (pet + 1 == 2)
                Console.Write("коровы");
            if (pet + 1 == 3)
                Console.Write("тигра");
            if (pet + 1 == 4)
                Console.WriteLine("зайца");
            if (pet + 1 == 5)
                Console.Write("дракона");
            if (pet + 1 == 6)
                Console.Write("змеи");
            if (pet + 1 == 7)
                Console.Write("лошади");
            if (pet + 1 == 8)
                Console.Write("овцы");
            if (pet + 1 == 9)
                Console.Write("обезьяны");
            if (pet + 1 == 10)
                Console.Write("курицы");
            if (pet + 1 == 11)
                Console.Write("собаки ");
            if (pet + 1 == 12)
                Console.Write("свиньи");
        }
    }
