            if (C =="С")
            {
                if (N == 0)
                {
                    Console.WriteLine("Направление Север");
                }
                else if (N == 1)
                {
                    Console.WriteLine("Направление Запад");
                }
                else if (N == -1)
                {
                    Console.WriteLine("Направление Восток");
                }
            }
